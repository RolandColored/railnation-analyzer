	var translations = {"europe":"Europe","tr":"Turkey","de":"Germany","cz":"Czech Republic","es,br,ar,pt":"Spain \/ Brazil \/ Argentina \/ Portugal","ru":"Russia","pl":"Poland","jp":"Japan","en":"International","nl":"Netherlands","fr":"France","dk,no,se,fi":"Denmark \/ Norway \/ Sweden \/ Finland","ua":"Ukraine","it":"Italy","ro":"Romania","gr,it":"Greece \/ Italy","ptr":"Public Test Realm","us":"USA","uk":"United Kingdom"}; 
	
	var selectedRegion = null;
	var selectedRegionName = null;
	var selectedCountry = null;
	var selectedCouontryName = null;
	var selectedServer = null;

	var preSelect = {"region":"europe","country":"en"};

	function formatToDate(timestamp, withTime) {
		if(typeof(withTime) == 'undefined') {
			withTime = false;
		}
		var date = new Date(timestamp*1000);
		var string = '';
		string = date.getDate()+'.'+(date.getMonth()+1)+'.'+date.getFullYear();
		if(withTime) {
			string += ' '+date.getHours()+':'+date.getMinutes()+':'+date.getSeconds();
		}
		return string;
	}
	
	function initServerSelection() {
		var first = '';
		var regionCount=0;
		for(var region in server) {
			var r = document.createElement('option');
			r.id ='region-'+region;
			r.className = 'region option';
			r.innerHTML = translations[region];
			r.value = region;
			if(preSelect != null && region == preSelect.reion) {
				r.selected = true;
			}
			$('#regions')[0].appendChild(r);
			if(first == '') first = region;
			regionCount++;
		}
		if(regionCount <= 1) {
			$('#regionContainer').css('display', 'none');
		} else {
			$('#regionContainer').css('display', 'block');
		}
		if(preSelect != null && typeof(server[preSelect.region]) != 'undefined') {
			selectRegion(preSelect.region);
		} else {
			selectRegion(first);
		}
		$('#serverTpl').css('display', 'none');
	}

	function selectRegion(regionName) {
		if(typeof(regionName) == 'undefined') {
			//Select country from select value
			regionName = $('#regions').val();
		}
		var region = server[regionName];
		if(typeof(region) == 'undefined') {
			console.log("Region not found "+regionName);
			return false;
		}
		if(selectedRegion != null) {
			$('#region-'+selectedRegionName).removeClass('active');
		}
		selectedRegion = region;
		selectedRegionName = regionName;
		$('#region-'+selectedRegionName).addClass('active');
		$('#countries')[0].innerHTML = '';
		var first = '';
		var i=0;
		for(var country in region) {
			var c = document.createElement('option');
			c.id = 'country-'+country;
			c.className = 'country option';
			c.value = country;
			if(preSelect != null && country == preSelect.country) {
				c.selected = true;
			}
			c.innerHTML = translations[country];
			$('#countries')[0].appendChild(c);
			if(first == '') first = country;
			i++;
		}
		if(i <= 1) {
			$('#countryContainer').css('display', 'none');
		} else {
			$('#countryContainer').css('display', 'block');
		}
		if(preSelect != null && typeof(region[preSelect.country]) != 'undefined') {
			selectCountry(preSelect.country);
		} else {
			selectCountry(first);
		}
		return true;
	};

	function selectCountry(countryName) {
		if(typeof(countryName) == 'undefined') {
			//Select country from select value
			countryName = $('#countries').val();
		}
		var country = selectedRegion[countryName];
		if(typeof(country) == 'undefined') {
			console.log("Country not found "+selectedRegion+" - "+countryName);
			return false;
		}
		selectedCountry = country;
		selectedCountryName = countryName;
		
		$('#servers')[0].innerHTML = '';
		var worlds = country['aviableServers'];
		var i=0;
		for(var worldName in worlds) {
			i++;
			var world = worlds[worldName];
			var row =  createServerRow(worldName, world);
			$('#servers').append(row);
		}	
		if(i==0) {
			var row = document.createElement('tr');
			row.className = 'server newServer';
			row.innerHTML = '<td colspan=5>Sorry, there is currently no server available in this country.</td>';
			$('#servers')[0].appendChild(row);
		}
	};

	function createLabelValueDiv(label, value, className) {
		var div = document.createElement('div');
		div.className = className;
		var labelSpan = document.createElement('span');
		labelSpan.className='serverLabel';
		labelSpan.innerHTML = label;
		div.appendChild(labelSpan);
		var valueSpan = document.createElement('span');
		valueSpan.className='serverValue';
		valueSpan.innerHTML = value;
		div.appendChild(valueSpan);
		return div;
	}

	function addColumn(row, content, className) {
		var column = document.createElement('td');
		if(typeof(content) == 'object') {
			column.appendChild(content);
		} else {
			column.innerHTML = content;
		}
		if(typeof(className) != 'undefined') {
			column.className = className;
		}
		row.appendChild(column);
	}

	function createServerRow(worldName, server) {
		var tpl = $('#server_id_'+server['consumers_id']);
		return tpl.clone();
	}

	function createAvatar(worldName) {
		var server = selectedCountry['aviableServers'][worldName];
		if(typeof(server) == 'undefined') {
			console.log(selectedCountry['aviableServers']);
			console.log('could not find: '+worldName);
			return;
		} 
		selectedServer = server;
		$('#createAvatarWorldInput').val(selectedServer['consumers_id']);
		if(selectedServer['register_with_avatar_name'] == 1 || selectedServer['world_status'] == 1) {
			//Mï¿½ssen das Formular noch anzeigen.
			$('#createAvatarWorldSoloInput').val(selectedServer['consumers_id']);
			$('#createAvatarFormHeadlineValue').html(selectedServer['world_name']);
			$('#worldSelection').css('display', 'none');
			if(selectedServer['register_with_avatar_name'] == 1){
				$('#avatarNameInput').css('display', 'block');
			} else {
				$('#avatarNameInput').css('display', 'none');
			}
			if(selectedServer['world_status'] == 1) {
				$('#betaCodeInput').css('display', 'block');
			} else {
				$('#betaCodeInput').css('display', 'none');
			}
			$('#createAvatarFormSoloContainer').css('display','block');
		} else {
			//Wir haben alle Daten
			$('#createAvatarForm').submit();
		}
	};

	function login(elem) {
		$(elem).find('.loginAvatarForm').submit();
	};

	function register(elem) {
		$(elem).find('.createAvatarForm').submit();
	};

	$(document).ready(initServerSelection);

</script>

