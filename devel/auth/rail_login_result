<body onload="$('input, textarea').placeholder();" class="lang en">
	<div class="page_margins">
		<div class="page">
			<div id="main">
				<div id="col3" role="main">
					<div id="col3_content" class="clearfix">
						<script type="text/javascript">
	if(typeof(window.localStorage)!="undefined" && window.localStorage != null){
		var session = '';
		window.localStorage.session = JSON.stringify(session);
	}

	var waiting = [];
	function addWaitingFor(name) {
		waiting.push(name);
	}
	function removeWaitingFor(name) {
		for(var i=0; i <= waiting.length; i++) {
			if(waiting[i] == name) {
				waiting.splice(i, 1);
				break;
			}
		}
		if(waiting.length == 0) {
			window.setTimeout(forward, 200);
		}
	}

	function mBoxDone() {
		removeWaitingFor('mbox');
	}

	function forceWaitingFinish () {
		$.post('/ajax/confirm-iframe/',{'iframeUrl' : '', 'loaded': 0}, function(data) {
			window.setTimeout(forward, 100);
		});
	}

	function forward() {
				if(window.top != window.parent) {
			//If sam iframe is inside an iframe access to window.parent.location.href is blocked by browser.
			//Workaround: instead of url forwarding the window.parent we forward our own location.
			//Only known intended occurence inside sam integrated vbb forwards.
			document.location.href="http://www.rail-nation.com/world.php";
		} else {
			window.parent.location.href="http://www.rail-nation.com/world.php";
		}
			}
			forward();
	</script>
<div class="type-text success forward">
			<span class="forwardText">If forward is not working</span>
	<a href="http://www.rail-nation.com/world.php" target="_parent" class="forwardLink">Please click here</a>
</div>					</div>
				</div>
			</div>
			<div id="footer" role="contentinfo"></div>
		</div>
	</div>
