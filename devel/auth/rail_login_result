							<script>
			function closeSelection() {
				var div = document.getElementById('selectBoxContainer');
				div.style.display = 'none';
			}
			
			function openSelection(event) {
				var div = document.getElementById('selectBoxContainer');
				div.style.display = 'inline-block';
				document.onclick = closeSelection;
			    event.stopPropagation();
			    if(typeof(window.event) != 'undefined')
			    	window.event.cancelBubble = true;
			}
	
			function selectServer(identifier) {
				var sel = document.getElementById('selection');
				//var value = sel.options[sel.selectedIndex].value;
				var value = identifier;
				var targetDiv = document.getElementById('selectedServer');
				targetDiv.innerHTML = servers[value];
				var targetInput = document.getElementById('chosenServer');
				targetInput.value = value;
				closeSelection();
			}
	
			function $el(id) {
				return document.getElementById(id);
			}

		 	$(function() {
				$(".tabs").tabs();
			});

		 	window.onerror = function(message, url, lineNo) {
		 		var data = {'exception': message, 'url': url, 'line': lineNo};
		 		$.ajax({
		 		        url: 'https://railnation-sam.traviangames.com//error/log',
		 		        dataType: 'json',
		 		        data: data,
		 		        type: 'POST'
		 			});
	 			return false;
		 	};
		</script>
